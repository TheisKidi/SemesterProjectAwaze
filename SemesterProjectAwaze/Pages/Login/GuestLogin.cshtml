@page
@model SemesterProjectAwaze.Pages.Login.GuestLoginModel
@{
}

<h1>LOG IND GÆST</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="@Model.Email" class="control-label">Email</label> <br />
        <input asp-for="@Model.Email" class="form-control"> <br />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Password" class="control-label">Password</label> <br />
        <input asp-for="@Model.Password" class="form-control"> <br />
    </div>
    <div class="form-group">
        <label asp-for="@Model.ConfirmPassword" class="control-label">Confirm Password</label> <br />
        <input asp-for="@Model.ConfirmPassword" class="form-control"><br />
    </div>
    <div class="form-group">
        <input type="submit" value="Login" class="btn btn-primary" />
    </div>
</form>