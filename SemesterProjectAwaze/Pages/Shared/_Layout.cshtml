                </div>
                <!-- NAVIGATION MENU -->
                <ul class="nav-links">
                    <!-- CHECKBOX HACK -->
                    <input type="checkbox" id="checkbox_toggle" />
                    <label for="checkbox_toggle" class="hamburger">&#9776;</label>

                    <!-- NAVIGATION MENUS -->
                    <div class="menu">
                    <li class="navbar-text-box"><a asp-page="/Sites/CreateProperty">Udlej dit sommerhus</a></li>
                    <li class="navbar-seperator">
                        <svg xmlns="http://www.w3.org/2000/svg" width="1" height="16" viewBox="0 0 1 16">
                            <line id="Line_10" data-name="Line 10" y2="16" transform="translate(0.5)" fill="none" stroke="#2c2b2b" stroke-width="1" />
                        </svg>
                    </li>
                    <li><a asp-page="/Sites/Browse">Feriehuse</a></li>
                    <li class="navbar-seperator">
                        <svg xmlns="http://www.w3.org/2000/svg" width="1" height="16" viewBox="0 0 1 16">
                            <line id="Line_10" data-name="Line 10" y2="16" transform="translate(0.5)" fill="none" stroke="#2c2b2b" stroke-width="1" />
                        </svg>
                    </li>
                    <li><a asp-page="/Sites/About">Om Awaze</a></li>
                    <li class="navbar-seperator">
                        <svg xmlns="http://www.w3.org/2000/svg" width="1" height="16" viewBox="0 0 1 16">
                            <line id="Line_10" data-name="Line 10" y2="16" transform="translate(0.5)" fill="none" stroke="#2c2b2b" stroke-width="1" />
                        </svg>
                    </li>
                    <li><a asp-page="/Login/Logout">Hjælp</a></li>
                    <li class="navbar-seperator">
                        <svg xmlns="http://www.w3.org/2000/svg" width="1" height="16" viewBox="0 0 1 16">
                            <line id="Line_10" data-name="Line 10" y2="16" transform="translate(0.5)" fill="none" stroke="#2c2b2b" stroke-width="1" />
                        </svg>
                    </li>
                    
                    
                        @{
                            if (SemesterProjectAwaze.Services.LoginService.MyId == "")
                            {
                            <li><a asp-page="/Login/HouseOwnerLogin">Husejer-login</a></li>
                            <li class="navbar-seperator">
                                <svg xmlns="http://www.w3.org/2000/svg" width="1" height="16" viewBox="0 0 1 16">
                                    <line id="Line_10" data-name="Line 10" y2="16" transform="translate(0.5)" fill="none" stroke="#2c2b2b" stroke-width="1" />
                                </svg>
                            </li>
                            <li><a asp-page="/Login/GuestLogin">Gæste-login</a></li>
                            <li class="navbar-seperator">
                                <svg xmlns="http://www.w3.org/2000/svg" width="1" height="16" viewBox="0 0 1 16">
                                    <line id="Line_10" data-name="Line 10" y2="16" transform="translate(0.5)" fill="none" stroke="#2c2b2b" stroke-width="1" />
                                </svg>
                            </li>
                            }
                            else if (SemesterProjectAwaze.Services.LoginService.MyId != "" && !SemesterProjectAwaze.Services.LoginService.iAmOwner)
                            {
                                <li><a asp-page="/Login/Logout">Log ud</a></li>
                                <li class="navbar-seperator">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="1" height="16" viewBox="0 0 1 16">
                                        <line id="Line_10" data-name="Line 10" y2="16" transform="translate(0.5)" fill="none" stroke="#2c2b2b" stroke-width="1" />
                                    </svg>
                                </li>
                                <li>
                                    <a asp-page="/Login/GuestProfile">
                                        <img src="~/img/accountIcon.svg" style="width: 25px; position: absolute; top: 23px;" />
                                    </a>
                                </li>
                            }
                            else if (SemesterProjectAwaze.Services.LoginService.MyId != "" && SemesterProjectAwaze.Services.LoginService.iAmOwner)
                            {
                                <li><a asp-page="/Login/Logout">Log ud</a></li>
                                <li class="navbar-seperator">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="1" height="16" viewBox="0 0 1 16">
                                        <line id="Line_10" data-name="Line 10" y2="16" transform="translate(0.5)" fill="none" stroke="#2c2b2b" stroke-width="1" />
                                    </svg>
                                </li>
                                <li>
                                    <a asp-page="/Login/HouseOwnerProfile">
                                        <img src="~/img/accountIcon.svg" style="width: 25px; position: absolute; top: 23px;"/>
                                        </a>
                                </li>
                            }
                        }
                    </div>
                </ul>
            </div>
        </nav>
    </header>
@RenderBody()
<footer class="border-top footer text-muted">
    <div class="footer-text">
        <div class="footer-header-container">
            <p class="footer-header">Awaze A/S, Virumgårdsvej 27, 2830 Virum - Danmark CVR.nr. 17484575</p>
        </div>
        <div class="footer-links-container">
            <ul class="footer-links">
                <li>
                    <svg id="denmarkFlag" xmlns="http://www.w3.org/2000/svg" width="45" height="21.42" viewBox="0 0 45 21.42">
                        <rect id="Rectangle_12" data-name="Rectangle 12" width="45" height="21.42" rx="1" fill="#c71e54" />
                        <rect id="Rectangle_13" data-name="Rectangle 13" width="3.96" height="21.42" transform="translate(8.56)" fill="#fff" />
                        <rect id="Rectangle_14" data-name="Rectangle 14" width="3.96" height="45" transform="translate(45 8.73) rotate(90)" fill="#fff" />
                    </svg>
                </li>
                <li><a asp-page="/Sites/Browse">Feriehuse</a></li>
                <li><a asp-page="#">Politik vedr. persondata</a></li>
                <li><a asp-page="#">Cookie Policy</a></li>
                <li><a asp-page="#">Generelle vilkår</a></li>
                <li><a asp-page="#">Lejebetingelser</a></li>
                <li><a asp-page="#">Værd at vide</a></li>
                <li><a asp-page="#">Agent login</a></li>
            </ul>
        </div>
    </div>
</footer>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
<script src="~/js/SelectHouseType.js" asp-append-version="true"></script>
<script src="~/js/Slider.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>